    "a68b65f6-fb9a-4ab2-b71e-ca134410e138": {
      "type": "text-and-image",
      "settings": {
        "image": "shopify:\/\/shop_images\/02_69f20348-45a5-489e-b1d3-0ccea68d45b0.gif",
        "image_width": 700,
        "subtitle": "",
        "title": "",
        "text": "<h3><strong>MODULAR DESIGN<\/strong><\/h3><p>Modular sofas adapt as your needs change. Reconfigure the layout, add new components, or take sections apart to move through tight spaces. With easy DIY assembly, you can construct and reconstruct our modular sofas with absolute flexibility.<\/p>",
        "button_label": "",
        "button_link": "",
        "button_label2": "",
        "button_link2": "",
        "button_style": "primary",
        "align_text": "left",
        "layout": "right",
