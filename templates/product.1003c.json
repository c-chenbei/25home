    "708b5192-fc51-4d11-9912-d9f7d78e8b18": {
      "type": "text-and-image",
      "settings": {
        "image": "shopify:\/\/shop_images\/12_9c054be5-9544-4b6c-9098-79ba194ee449.jpg",
        "image_width": 700,
        "subtitle": "",
        "title": "",
        "text": "<h3><strong>SL Curve Track Design<\/strong><\/h3><p>Unique SL curve track design for integrated neck, shoulder, waist, and hip massage, meeting the all-around, three-dimensional massage needs of the body. Simulated human hand massage path precisely covers every inch of skin from head to toe, providing a comprehensive and thoughtful massage experience.<\/p>",
        "button_label": "",
        "button_link": "",
        "button_label2": "",
