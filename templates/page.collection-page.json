        "683c7b62-3e79-48bf-b73b-cc1ad6bb8a40": {
          "type": "feathers_box",
          "settings": {
            "one_img": "shopify:\/\/shop_images\/Disassemble.gif",
            "one_link": "",
            "one_title": "Washable Cover",
            "one_content": "The thoughtful zipper design allows the cover of the Plume couch to be removed, making it easy to wash and clean. The covers are made of a breathable fabric that is machine washable, keeping your feather sofa fresh.",
            "two_img": "shopify:\/\/shop_images\/modular_design.gif",
            "two_link": "https:\/\/25home.com\/collections\/plume-sofa",
            "two_title": "Modular Design",
            "two_content": "Our Plume feather sofa has a modular design so you can customize your configuration to perfectly fit your space.",
            "three_img": "shopify:\/\/shop_images\/BETTER_MATERIALS.gif",
            "three_link": "",
            "three_title": "Kids & Pets Friendly",
            "three_content": "The Feathers Sectional Sofa offers hassle-free cleaning with removable, washable covers. Its durable, high-quality fabric withstands kids and pets without you worrying about wear and tear. The scratch-resistant material and high-density, feather-filled cushions embrace you in relaxation and comfort for years."
          }
        },
        "ee1b0c34-e13a-4aa0-a84f-c611bc228bc6": {
          "type": "feathers_gongyi",
          "settings": {
            "gongyi_img": "shopify:\/\/shop_images\/gongyi.png",
            "gongyi_mobile_img": "shopify:\/\/shop_images\/750-__07.jpg",
            "gongyi_title": "Unique Sturdy&Built to Last Over a Decade",
            "gongyi_content": "Our Feathers sofa provides up to 10 years of unwavering comfort and support thanks to its premium materials and expert craftsmanship. Sink into plush relaxation you can enjoy today and for years to come. From the chrome-plated iron latch to the wood frame, every part of our furniture is well-chosen for durability. The maximum weight capacity is 500 pounds per seat."
          }
        },
        "2d19ee57-7be1-460c-a28f-e638bae1b57d": {
          "type": "feathers_Diverse",
          "settings": {
            "Diverseone_img": "shopify:\/\/shop_images\/Diverse_Colors.gif",
            "Diverseone_mobile_img": "shopify:\/\/shop_images\/GIF_cbb815b7-79a5-4adb-8399-880536743607.gif",
            "Diverseone_link": "https:\/\/25home.com\/collections\/plume-sofa",
            "one_title": "Versatile Sofa Legs",
            "one_content": "Free Sofa Legs Included! The sofa legs come with 2 installation options: flush with the ground or 3 inches above the ground, providing convenience for using a robotic vacuum cleaner and other tools for easy cleaning.",
            "Diversetwo_img": "shopify:\/\/shop_images\/Versatile_Sofa_Legs.gif",
            "Diversetwo_mobile_img": "shopify:\/\/shop_images\/aa2eb0bae1e780549d8c7e1ac0ed68b2.gif",
            "Diversetwo_link": "https:\/\/25home.com\/collections\/plume-sofa",
            "two_title": "Diverse Colors",
            "two_content": "Choose from 9 stylish colors to find the perfect match for your home's decor. From rich bold tones to airy neutrals, our spectrum of shade options ensures there’s a Feathers sofa to complement your personal taste and space. And we offer free color swatches.",
            "Diversethree_img": "shopify:\/\/shop_images\/Easy_Assembly.png",
            "Diversethree_mobile_img": "shopify:\/\/shop_images\/Easy_Assembly.png",
            "Diversethree_link": "https:\/\/25home.com\/collections\/plume-sofa",
            "three_title": "Easy Assembly 10- 15 Mins",
            "three_content": "With just a 'click,' our special toggle latch design ensures furniture assembly is as effortless as building with LEGO blocks. A one-step solution that frees your hands, without the burden of instruction manual."
          }
        },
