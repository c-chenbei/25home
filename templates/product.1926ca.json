    "cddf7ffe-0b62-4ce8-abdb-e2f6e07b9895": {
      "type": "text-and-image",
      "settings": {
        "image": "shopify:\/\/shop_images\/oeko-tex-600.jpg",
        "image_width": 700,
        "subtitle": "",
        "title": "",
        "text": "<h3><strong>OEKO-TEX Certified<\/strong><\/h3><p>Prioritize your health and the environment with our OEKO-TEX certified cover fabrics. Free from \"forever chemicals,\" toxic flame retardants, formaldehyde, and harmful components, our covers offer a safe and eco-friendly choice.<\/p>",
        "button_label": "",
        "button_link": "",
        "button_label2": "",
        "button_link2": "",
        "button_style": "primary",
        "align_text": "left",
        "layout": "left",
